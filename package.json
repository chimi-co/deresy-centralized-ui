{
  "name": "deresy-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:staging": "vue-cli-service build --mode staging",
    "deploy:dev": "yarn build:dev && firebase deploy -P development --only hosting",
    "deploy:staging": "yarn build:staging && firebase deploy -P staging --only hosting",
    "deploy:prod": "yarn build && firebase deploy -P production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "uglify": "./node_modules/.bin/uglifyjs $1 --compress --output ./dist/uglyfy/$1"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@kyvg/vue3-notification": "^2.3.4",
    "@metamask/detect-provider": "^1.2.0",
    "@portis/web3": "^4.0.5",
    "@vuelidate/core": "^2.0.0-alpha.42",
    "@vuelidate/validators": "^2.0.0-alpha.30",
    "axios": "^0.21.4",
    "configcat-js": "5.7.1",
    "core-js": "^3.6.5",
    "element-plus": "2.2.8",
    "firebase": "^8.8.0",
    "fortmatic": "^2.2.1",
    "marked": "4.3.0",
    "quill-delta-to-html": "^0.12.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "vuex-persist": "^3.1.3",
    "walletlink": "^2.1.8",
    "web3": "^1.5.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@types/p5": "^1.3.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^9.1.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "typescript": "~3.9.3",
    "uglify-js": "^3.14.3",
    "vue-jest": "^5.0.0-0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "git add"
    ]
  }
}
